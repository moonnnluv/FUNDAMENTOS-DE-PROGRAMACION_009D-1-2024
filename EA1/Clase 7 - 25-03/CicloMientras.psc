Proceso CicloMientras
	
	Definir k Como Entero;
	
	k = 5;
	
	Mientras k<10 Hacer
		Escribir k;
		k = k+1;
	FinMientras
	
	Limpiar Pantalla;
	
	// 100 primeros numeros pares
	
	k = 1;
	
	Mientras k <= 100 Hacer
		Escribir 2*k;
		k = k+1;
	FinMientras
	
	Limpiar Pantalla;
	
	// 50 primeros numeros impares
	
	k = 1;
	
	Mientras k <= 50 Hacer
		Escribir 2*k-1;
		k = k+1;
	FinMientras
	
	
	
	
	
FinProceso
